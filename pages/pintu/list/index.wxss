/* pages/pintu/list/index.wxss */
@import '/assets/styles/background.wxss';
page{
  padding-top: 10vh;
  height: 90vh;
}
.wenli-bg {
  height: 80vh;
}
.container{
  padding: 25rpx;
  width: 700rpx;
}

.cate-title-bg{
  background: #e9eef5 url('data:image/png;base64,') no-repeat;
  background-size: 100%;
  background-repeat: no-repeat;

}
.cate-title{
  /* position: relative; */
  width: 50rpx;
}
.cate-title > text{
  display: block;
  position: absolute;
  writing-mode:vertical-lr;
  color: #FFF;
  padding-top: 40rpx;
  text-align: center;
}

.cate-title > image{
  display: block;
  position: absolute;
  width: 40rpx;
  height: 180rpx;
}


.pintu-cate-item{
  display: flex;
  flex-direction: row;
}

.cate-list{
  display: flex;
  flex-direction: row;
  margin-bottom: 50rpx;
}

.cate-list-item{
  display: flex;
  flex-direction: column;
  margin-right: 10rpx;
}
.cate-list-item>image{
  width: 140rpx;
  height: 180rpx;
}

.cate-list-item>text{
  display: block;
  background-color: #FFF;
  border:#e9eef5 5rpx solid;
  border-radius: 20rpx;
  width: 100rpx;
  text-align: center;
  margin: 0 auto;
}
/* pages/learning/detail/index.wxss */

@import '/assets/styles/background.wxss';

page{
  height: 95vh;
}

#video-panel{
  width: 700rpx;
  margin: 0 25rpx;
  margin-top: 30rpx;
  background-color: #FFF;
  border-top-left-radius: 25rpx;
  border-top-right-radius: 25rpx;
}

#video {
  width: 100%;
  text-align: center;
}

#video > video {
  margin: 0 auto;
  width: 700rpx;
  height: 400rpx;
  border-radius: 25rpx;
}

#button-list{
  display: flex;
}

#button-list>.button{
  width:33%;
  margin: 0 auto;
  text-align: center;
}

#button-list>.button>image{
  width: 88rpx;
  height: 92rpx;
}


#result-panel{
  width: 700rpx;
  margin: 0 25rpx;
  margin-top: 25rpx;
  padding-bottom: 25rpx;
  background-color: #FFF;
  display: flex;
  flex-wrap: wrap;
  border-bottom-left-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
}

#result-score{
  background: url('data:image/jpeg;base64,');
  width: 206rpx;
  height: 206rpx;
  margin: 20rpx;
  border: 1rpx solid #8e2a2a;
  text-align: center;
  background-size: 100% 100%;
}

#result-score>text{
  display: block;
  font-size: 96rpx;
  padding-top: 40rpx;
  color: chocolate;
}

#result-wave{
  width: 450rpx;
}

.wave{
  /* position: absolute;
  left: 0;
  top: 0;
  z-index: 9999; */
  /* width: 100vw; */
  width: 432rpx;

  height: 206rpx;
  overflow: hidden;
}
.canvas {
  /* width: 100vw; */
  width: 100%;
  height: 206rpx;
  background-color: #ccc;
}

#wave_parent{
  margin: 20rpx;
  margin-left: 0rpx;
}

#result-suggestion{
  width: 660rpx;
  height: 220rpx;
  background-color: #e9eef5;
  margin: 0 20rpx;
}

#result-suggestion>image, #result-suggestion>text{
  display: block;
}

#result-suggestion>.content{
  margin-top: 10rpx;
  margin-left: 20rpx;
}

#allocAuth{
  width: 700rpx;
  margin: 10rpx 25rpx;
}


/* pages/questions/dati/index.wxss */
@import '/assets/styles/background.wxss';

#dati-top{
  text-align: center;
  padding-top: 100rpx;
  height: 300rpx;
}

#dati-main{
  padding: 0 90rpx;
}
#dati-main>text{
  display: block;
  margin-top: -200rpx;

}
#dati-container{
  height: 1140rpx;
  position: relative;
  /* padding-top: 5vh; */
}
#dati-container > .bg, #dati-container > .bg > image{
  width: 678rpx;
  height: 1140rpx;
}

page{
  height: 90vh;
  padding-top: 5vh;
}

#dati-container{
  width: 678rpx;
  margin: 0 auto;
}

#dati-container > .main{
  position: absolute;
  top: 120rpx;
  width: 678rpx;
}

.main{
  
}

.bottom{
  position: absolute;
  bottom: 100rpx;
  width: 678rpx;
}

.main > .num{
  text-align: center;
}

.main > .num> text{
  font-size: 36rpx;
  font-weight: bold;
  color: #953737;
}

.main > .content{
  margin: 0 69rpx;
  width: 540rpx;
  margin-top: 60rpx;

}
.main > .content > text{
  display: block;
}
.main > .content > .image{
  margin-top: 10rpx;
  display: block;
  text-align: center;
}
.main > .content > .image > image{
  width: 250rpx;
  height: 300rpx;
}

.options{
  margin: 0 69rpx;
  width: 520rpx;
  padding: 0 10rpx;
}

.checked{
  background-color: bisque;
}

.options>.option-item{
  margin-bottom: 10rpx;
}

.btngroup{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}

.dati-btn{
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
  width: 157rpx;
  height: 78rpx;
}

.btn-item{
  width: 50%;
  text-align: center;
  margin-left: 80rpx;
}

.dati-btn > text{
  display: block;
  padding-top: 30rpx;
  text-align: center;
}
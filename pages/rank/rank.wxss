/* pages/rank/rank.wxss */

@import '/assets/styles/background.wxss';

#top-nav{
  text-align: center;
  display: flex;
  margin-top: 45rpx;
}

#top-nav>view{
  width: 50%;
}

#rank{
  background-color: #FFF;
  width: 700rpx;
  margin: 0 auto;
}

#top-msg-panel{
  display: flex;
  flex-direction: row;
  margin: 10rpx 25rpx;
  background-color: #FFF;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  padding: 50rpx;

    background: #FFF url('data:image/png;base64,') no-repeat center 40rpx;
    background-size: 40%;
    padding-top: 240rpx;
}


#rank{
  margin-top: 20rpx;
}

#top-user{
  height: 220px;
  display: flex;
  width: 700rpx;
  padding-top: 25rpx;
  /* margin: 0 17rpx; */
}
#top-user>.top-user-item{
  width: 220rpx;
  margin: 0 8rpx;
  background-color: palegoldenrod;
  border-radius: 5px;
  margin-top: 10rpx;
}
#top-user>.top-user-item:nth-child(2){
  width: 230rpx;
  margin-top: 0rpx;
  padding-top: 10rpx;
  background-color: rgb(223, 217, 156);
}

#top-user>.top-user-item>view{
  margin: 0 auto;
}
#top-user>.top-user-item>.item-index>text{
  margin: 0 auto;
  margin-top: 10px;
  display: block;
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: orange;
  color: #FFF;
}
#top-user>.top-user-item>.item-avatar{
  margin: 10px 0;
  text-align: center;
}
#top-user>.top-user-item>.item-avatar>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}

#top-user>.top-user-item>.detail-userName{
  text-align: center;
  font-weight: bolder;

}
#top-user>.top-user-item>.detail-userName>text{
  line-height: 1em;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  overflow: hidden;

}

#top-user>.top-user-item>.item-point{
  text-align: center;
  color: red;
  font-weight: bolder;
}

#top-user>.top-user-item>.detail-school{
  text-align: center;
  font-size: 0.8em;
}

.item{
  border-bottom: 1px solid #ccc;
  height: 60px;
  padding: 10px 0;
  margin: 0 20rpx;
}
.item>view{
  float: left;
  height: 60px;
}
.item>.item-index{
  width: 80rpx;
  text-align: center;
  line-height: 100px;
}
.item>.item-index>text{
  margin: 10px auto;
  display: block;
  background-color: #999;
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
  line-height: 50rpx;
  color: #FFF;
}
.item>.item-avatar{
  width: 140rpx;
}
.item>.item-avatar>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  border: 1px solid orange;
}
.item>.item-detail{
  width: 310rpx;
  padding-top: 10rpx;
}
.item>.item-detail>.detail-userName{
  font-size: 1em;
  font-weight: bold;
}
.item>.item-detail>.detail-school{
  font-size: 0.8em;
}

.item>.item-point{
  width: 130rpx;
  color: #e60000;
  line-height: 100rpx;
  font-size: 1.4em;
  text-align: right;
}